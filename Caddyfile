{
    admin off
    auto_https off
}

viken.stream:8443 {
    tls /etc/letsencrypt/live/viken.stream/fullchain.pem /etc/letsencrypt/live/viken.stream/privkey.pem

    @livekit {
        path /rtc* /twirp* /livekit* /ws
    }
    reverse_proxy @livekit livekit:7880 {
        transport http {
            versions h2c 1.1
        }
    }

    @token {
        path /token*
    }
    reverse_proxy @token token-server:8000

    root * /srv/www
    file_server
}
